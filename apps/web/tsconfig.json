{
  "extends": "@repo/typescript-config/nextjs.json",
  "compilerOptions": {
     "types": ["next-auth"],
    "plugins": [
      {
        "name": "next"
      }
    ],
     "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    "next.config.js",
    ".next/types/**/*.ts",
    "types/**/*.d.ts"
, "langgraph/nodes.mts"  ],
  "exclude": [
    "node_modules"
  ]
}
